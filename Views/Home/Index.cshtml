@model List<Registrar>



<h1><center>RESULTADO</center></h1>

  <table class="table table-bordered">
    
  <thead class="thead-dark">
    <tr>
      <th scope="col">N°</th>
      <th scope="col">Producto</th>
      <th scope="col">url</th>
      <th scope="col">Descripcion</th>
      <th scope="col">Precio</th>
      <th scope="col">Direccion</th>
      <th scope="col">Usuario</th>
      <th scope="col">Telefono</th>
      <th scope="col">Fecha</th>
     <th scope="col">Administrar</th>
    </tr>
  </thead>
@foreach (var result in Model)
        {
          <tr>
                <td>@Html.DisplayFor(modelItem => result.id)</td>
                <td>@Html.DisplayFor(modelItem => result.Nombre)</td>
                <td>@Html.DisplayFor(modelItem => result.url)</td>
                <td>@Html.DisplayFor(modelItem => result.Descripcion)</td>
                 <td>@Html.DisplayFor(modelItem => result.Precio)</td>
                  <td>@Html.DisplayFor(modelItem => result.Direccion)</td>
                  <td>@Html.DisplayFor(modelItem => result.Usuario)</td>
                   <td>@Html.DisplayFor(modelItem => result.Telefono)</td>
                    <td>@Html.DisplayFor(modelItem => result.Fecha)</td>
                <td><a asp-action="Editar" asp-route-id="@result.id" class="badge badge-primary">Editar</a></td>                
            </tr>
            
        }
</table>